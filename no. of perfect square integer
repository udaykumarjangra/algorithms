//no. of square integer in a range

#include <bits/stdc++.h> 
using namespace std; 
  
bool isPerfectSquare(int x) 
{   
  // Find floating point value of  
  // square root of x. 
  long double sr = sqrt(x); 
  
  // If square root is an integer 
  return ((sr - floor(sr)) == 0); 
} 

int main()
{
	int a,b,i,count=0;
	cout<<"Enter the starting and ending integer of the range: \n";
	cin>>a>>b;
	
	for(i=a+1;i<=b;i++)
	{
		if(isPerfectSquare(i)==1)
		{
			count++;
		}
	}
	
	cout<<"The no. of square integers between "<<a<<" & "<<b<<" is "<<count;
	return 0;
}

